create table DT82(  MT82ID uniqueidentifier  not null , DT82ID uniqueidentifier  not null , MaNVien nvarchar(32) not null , LHD decimal(20,6), LNgayCong decimal(20,6), LDS decimal(20,6), LTC decimal(20,6), PCCom decimal(20,6), PCDHai decimal(20,6), PCCvu decimal(20,6), PCDThoai decimal(20,6), PCNha decimal(20,6), PCXang decimal(20,6), BHXH0 decimal(20,6), BHYT0 decimal(20,6), BHTN0 decimal(20,6), BHXH1 decimal(20,6), BHYT1 decimal(20,6), BHTN1 decimal(20,6), LThucNhan decimal(20,6), LGTBT decimal(20,6), LGTGC decimal(20,6), TNCThue decimal(20,6), ThueTNCN decimal(20,6), ThucLinh decimal(20,6), TamUng decimal(20,6), Conlai decimal(20,6), ws nvarchar(4000),Grws nvarchar(4000) )
