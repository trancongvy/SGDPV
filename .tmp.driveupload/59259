create table DT61(  MT61ID uniqueidentifier  not null , DT61ID uniqueidentifier  not null , MaVT nvarchar(32) not null , KLNT decimal(20,6), KLTram decimal(20,6), MaKH nvarchar(32), MaCongTrinh nvarchar(32), MaXe int IDENTITY , MaTram nvarchar(32), MaKyThuatVien nvarchar(32), ws nvarchar(4000),Grws nvarchar(4000) )
ALTER TABLE DT61 ADD CONSTRAINT PK_DT61 PRIMARY KEY CLUSTERED (DT61ID)
