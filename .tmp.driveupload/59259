create table CtYTL1(  stt int IDENTITY  not null , Nam int , Thang int , BacLuong decimal(20,6), GTBT decimal(20,6), GTGC decimal(20,6), LuongBH decimal(20,6), LuongCB decimal(20,6), LuongGio decimal(20,6), LuongNgay decimal(20,6), LuongThang decimal(20,6), NgayCong decimal(20,6), OT15 decimal(20,6), OT20 decimal(20,6), OT30 decimal(20,6), PCCom decimal(20,6), PCCThoai decimal(20,6), PCCVu decimal(20,6), PCDHai decimal(20,6), PCNha decimal(20,6), TDS decimal(20,6), ws nvarchar(4000),Grws nvarchar(4000) )
