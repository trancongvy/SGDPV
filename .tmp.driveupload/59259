create table MT3B(  MT3BID uniqueidentifier  not null , sysDBID int  not null , MaCT nvarchar(128) not null , NgayCT smalldatetime, SoCT nvarchar(128), MaCN nvarchar(32), TTienH decimal(20,6), TThue decimal(20,6), TTien decimal(20,6), MaKH nvarchar(32), TienKT decimal(20,6), TThua decimal(20,6), MaNVien nvarchar(32), TaskID uniqueidentifier , Approved int , ws nvarchar(4000),Grws nvarchar(4000) )
ALTER TABLE MT3B ADD CONSTRAINT PK_MT3B PRIMARY KEY CLUSTERED (MT3BID)
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_sysDBID DEFAULT ('(@@sysDBID)') FOR sysDBID
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_MaCT DEFAULT ('BL') FOR MaCT
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_NgayCT DEFAULT ('n') FOR NgayCT
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_SoCT DEFAULT ('BLYYMM0001') FOR SoCT
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_MaCN DEFAULT ('(@@MaCN)') FOR MaCN
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_TTienH DEFAULT ('0') FOR TTienH
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_TThue DEFAULT ('0') FOR TThue
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_TTien DEFAULT ('0') FOR TTien
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_MaKH DEFAULT ('KLE-001') FOR MaKH
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_TienKT DEFAULT ('0') FOR TienKT
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_TThua DEFAULT ('0') FOR TThua
ALTER TABLE MT3B ADD CONSTRAINT DF_MT3B_Approved DEFAULT ('0') FOR Approved
